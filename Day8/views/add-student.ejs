<html>

<body>
    <h1>Add Student</h1>
    <form method="POST" action="/student-process">
        <table>
            <tr>
                <td><label> Name:</label></td>
                <td> <input type="text" name="txt1" required /></td>
            </tr>

            <tr>
                <td><label>Email:</label></td>
                <td><input type="email" name="txt2" required /></td>
            </tr>

            <tr>
                <td> <label> Age :</label></td>
                <td> <input type="number" name="txt3" required /></td>
            </tr>

            <tr>
                <td> <label> Marks :</label></td>
                <td> <input type="number" name="txt4" required /></td>
            </tr>

            <tr>
                <td> <label> Branch :</label></td>
                <td>
                    <select name="txt5">
                <option value="">-- Choose Branch--</option>
                <option value="it">IT</option>
                <option value="com">Computer</option>
                <option value="ec">EC</option>
                <option value="civ">Civil</option>
                <option value="ele">Electrical</option>
                <option value="auto">Auto</option>
             </select>
                </td>
            </tr>

            <tr>
                <td colspan="1"></td>
                <td><input type="submit" /></td>
            </tr>
        </table>
    </form>
</body>

</html>